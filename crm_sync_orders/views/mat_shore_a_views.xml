<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="mat_shore_a_action" model="ir.actions.act_window">
        <field name="name">MatShoreA</field>
        <field name="res_model">mat.shore.a</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="sale_mat_shore_a_menu" parent="sale.product_menu_catalog"
        action="mat_shore_a_action" sequence="1105"/>



    <record id="mat_shore_a_view_tree" model="ir.ui.view">
        <field name="name">MatShoreA List</field>
        <field name="model">mat.shore.a</field>
        <field name="arch" type="xml">
            <tree string="MatShoreA">
                <field name="value"/>
            </tree>
        </field>
    </record>


    <record id="mat_shore_a_view_form" model="ir.ui.view">
        <field name="name">MatShoreA Form</field>
        <field name="model">mat.shore.a</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="value"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
